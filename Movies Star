.widget ul li, .widget #ArchiveList ul.flat li { border-top: 1px dashed rgba(128, 128, 128, 0.5); }

.widget ul li:first-child, .widget #ArchiveList ul.flat li:first-child { border-top: none; }

.widget .post-body ul { list-style: disc; }

.widget .post-body ul li { border: none; }

.footer-outer { color: rgb(204, 204, 204); background: url("https://resources.blogblog.com/blogblog/data/1kt/transparent/black50.png") left top repeat scroll transparent; border-radius: 0px 0px 20px 20px; box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 3px; }

